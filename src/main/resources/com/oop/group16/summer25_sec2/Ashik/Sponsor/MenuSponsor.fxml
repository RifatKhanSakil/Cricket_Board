<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.oop.group16.summer25_sec2.Ashik.Sponsor.MenuSponsorController"
            prefHeight="500.0" prefWidth="520.0">
    <children>
        <VBox spacing="10.0" AnchorPane.topAnchor="20.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0">
            <children>
                <Label text="Sponsor Menu" style="-fx-font-size: 18px; -fx-font-weight: bold;"/>
                <Button text="Sponsorship &amp; Brand Visibility" onAction="#openBrandVisibility"/>
                <Button text="Promote Product Launches" onAction="#openPromoteProductLaunches"/>
                <Button text="International Tour Sponsorship" onAction="#openSponsorshipAmount"/>
                <Button text="Co-Branded Fan Event Management" onAction="#openCompanyEventManagement"/>
                <Button text="Social Media Impact" onAction="#openSocialMediaImpact"/>
                <Button text="Grassroots Development" onAction="#openGrassrootDevelopment"/>
                <Button text="Positive Brand Image" onAction="#openPositiveBrandImage"/>
                <Button text="Strategic Partnership Objectives" onAction="#openSponsorshipObjectives"/>
                <Button text="Back to Login" onAction="#BackToLoginButton"/>
            </children>
        </VBox>
    </children>
</AnchorPane>
